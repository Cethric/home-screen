{
  "$schema": "https://biomejs.dev/schemas/2.1.1/schema.json",
  "root": false,
  "extends": "//",
  "vcs": {
    "root": "../../../.git/"
  },
  "linter": {
    "rules": {
      "complexity": {
        "noUselessSwitchCase": "off"
      },
      "suspicious": {
        "noExplicitAny": "off",
        "noDoubleEquals": "off"
      },
      "style": {
        "noNonNullAssertion": "off"
      }
    }
  }
}
